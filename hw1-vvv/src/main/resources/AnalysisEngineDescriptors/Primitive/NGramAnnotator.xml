<?xml version="1.0" encoding="UTF-8"?>

<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>  <annotatorImplementationName>Annotators.NGramAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>NGramAnnotator</name>
    <description>The system will annotate 1-,2- and 3-grams of consecutive tokens.</description>
    <version>1.0</version>
    <vendor/>
    <configurationParameters/>
    <configurationParameterSettings/>
    <typeSystemDescription>
      <types>
        <typeDescription>
          <name>Types.TestElement.Sentence</name>
          <description>SuperType of Question and Answer.</description>
          <supertypeName>Types.Base.BaseAnnotation</supertypeName>
        <features>
            <featureDescription>
              <name>Id</name>
              <description>Unique number assigned to each sentence to allow token to determine which sentence it is part of.</description>
              <rangeTypeName>uima.cas.Integer</rangeTypeName>
            </featureDescription>
          <featureDescription>
              <name>TokenList</name>
              <description>List of tokens in sentence.</description>
              <rangeTypeName>uima.cas.FSArray</rangeTypeName>
              <elementType>Types.Processed.Token</elementType>
            </featureDescription>
          </features>
        </typeDescription>
        <typeDescription>
          <name>Types.Base.BaseAnnotation</name>
          <description>Common core attributes shared by all types.</description>
          <supertypeName>uima.tcas.Annotation</supertypeName>
        <features>
            <featureDescription>
              <name>CasProcessorId</name>
              <description>componentId of creator.</description>
              <rangeTypeName>uima.cas.String</rangeTypeName>
            </featureDescription>
            <featureDescription>
              <name>Confidence</name>
              <description>Confidence of the creator in the correctness of the output on a scale of 0 to 1.0.</description>
              <rangeTypeName>uima.cas.Float</rangeTypeName>
            </featureDescription>
          </features>
        </typeDescription>
      <typeDescription>
          <name>Types.TestElement.Answer</name>
          <description>Proposed answer to question being asked in sample information processing task.</description>
          <supertypeName>Types.TestElement.Sentence</supertypeName>
          <features>
            <featureDescription>
              <name>IsCorrect</name>
              <description>Boolean Flag indicating whether or not the proposed answer is correct.</description>
              <rangeTypeName>uima.cas.Boolean</rangeTypeName>
            </featureDescription>
          <featureDescription>
              <name>Score</name>
              <description>Answer score (assigned using Gold Answer Scoring System).</description>
              <rangeTypeName>uima.cas.Float</rangeTypeName>
            </featureDescription>
          </features>
        </typeDescription>
      <typeDescription>
          <name>Types.Processed.NGram</name>
          <description>Type representing unigram, bigram or trigram in question/answer.</description>
          <supertypeName>Types.Base.BaseAnnotation</supertypeName>
          <features>
            <featureDescription>
              <name>Elements</name>
              <description>Tokens in NGram.</description>
              <rangeTypeName>uima.cas.FSArray</rangeTypeName>
              <elementType>Types.TestElement.Answer</elementType>
            </featureDescription>
            <featureDescription>
              <name>ElementType</name>
              <description>Type of the Objects stored in 'elements' array.</description>
              <rangeTypeName>uima.cas.String</rangeTypeName>
            </featureDescription>
          </features>
        </typeDescription>
      <typeDescription>
          <name>Types.Processed.Token</name>
          <description>Token in question/answer (delimited by whitespace and punctuation).</description>
          <supertypeName>Types.Base.BaseAnnotation</supertypeName>
          <features>
            <featureDescription>
              <name>SentenceId</name>
              <description>Unique number assigned to each sentence to allow token to determine which sentence it is part of.</description>
              <rangeTypeName>uima.cas.Integer</rangeTypeName>
            </featureDescription>
            <featureDescription>
              <name>PartOfSpeech</name>
              <description>Linguistic category of token.</description>
              <rangeTypeName>uima.cas.String</rangeTypeName>
            </featureDescription>
            <featureDescription>
              <name>Text</name>
              <description>Text contained in token.</description>
              <rangeTypeName>uima.cas.String</rangeTypeName>
            </featureDescription>
          </features>
        </typeDescription>
      <typeDescription>
          <name>Types.Processed.Unigram</name>
          <description>An n-gram of size 1.</description>
          <supertypeName>Types.Processed.NGram</supertypeName>
        </typeDescription>
        <typeDescription>
          <name>Types.Processed.Bigram</name>
          <description>An n-gram of size 2.</description>
          <supertypeName>Types.Processed.NGram</supertypeName>
        </typeDescription>
        <typeDescription>
          <name>Types.Processed.Trigram</name>
          <description>An n-gram of size 3.</description>
          <supertypeName>Types.Processed.NGram</supertypeName>
        </typeDescription>
      </types>
    </typeSystemDescription>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">Types.TestElement.Sentence</type>
        </inputs>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
  <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
